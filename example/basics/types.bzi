require "std:foundation"

print("test")

print(Some(8))

print("tset")

fn Optional::map(self, f) =>
  match self {
    case Some(x) => Some(f(x))
    case None => None
  }

print(Optional::map(Some(8), fn(x) => x + 1))

let test = Ok(8)

print(match test {
  case Ok(x) if x > 7 => "ok"
  case Error(_) | Ok(_) => "not ok"
})
